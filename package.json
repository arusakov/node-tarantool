{
  "name": "node-tarantool",
  "version": "0.0.1",
  "description": "Tarantool v1.7+ driver for Node.js",
  "main": "./lib/node-tarantool.js",
  "typings": "./node-tarantool.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "compile": "tsc --noEmit -p ./src && tsc --noEmit -p ./test",
    "coverage": "cross-env NODE_ENV=test TS_NODE_PROJECT=./test nyc mocha",
    "coverage-html": "cross-env NODE_ENV=test TS_NODE_PROJECT=./test nyc --reporter=html mocha",
    "prepublish": "npm run clean && tsc -d --declarationDir ./ -p ./src",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=./test mocha",
    "test-ci": "cross-env  NODE_ENV=test TS_NODE_PROJECT=./test mocha -w",
    "travis": "npm run compile && npm run coverage && npm run codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arusakov/node-tarantool.git"
  },
  "keywords": [
    "Node",
    "Tarantool",
    "msgpack",
    "socket",
    "database",
    "db"
  ],
  "authors": [
    "Alexander Rusakov <a_s_rusakov@mail.ru> (https://github.com/arusakov)",
    "Artem Nikolsky (https://github.com/nikolsky)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arusakov/node-tarantool/issues"
  },
  "homepage": "https://github.com/arusakov/node-tarantool#readme",
  "devDependencies": {
    "bluebird": "3.4.6",
    "codecov": "^1.0.1",
    "cross-env": "3.0.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.1.0",
    "msgpack-lite": "0.1.20",
    "nyc": "8.3.0",
    "rimraf": "2.5.4",
    "ts-node": "1.3.0",
    "typescript": "2.0.3",
    "typings": "1.4.0"
  }
}
